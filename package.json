{
  "name": "@wework/wapiti",
  "version": "0.2.1",
  "description": "React UI for API Documentation supporting OpenAPI and gRPC",
  "keywords": [
    "api",
    "documentation",
    "openapi",
    "rest",
    "grpc",
    "service mesh",
    "kiali"
  ],
  "homepage": "./",
  "bugs": {
    "url": "https://github.com/WeWorkSandbox/wapiti/issues"
  },
  "main": "build/wapiti.js",
  "module": "build/wapiti.module.js",
  "scripts": {
    "build": "rollup -c rollup.config.babel.js",
    "watch": "rollup -c rollup.config.babel.js -w",
    "examples:build": "cd \"./examples\" && \"./node_modules/.bin/webpack\"",
    "examples:watch": "cd \"./examples\" && \"./node_modules/.bin/webpack-dev-server\"",
    "start": "(npm run watch & npm run examples:watch)",
    "test": "jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeWorkSandbox/wapiti.git"
  },
  "license": "Apache-2.0",
  "author": "WeWork",
  "dependencies": {
    "classnames": "2.2.6",
    "js-yaml": "3.13.1",
    "json-refs": "^3.0.13",
    "marked": "0.7.0",
    "react-dangerous-html": "1.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "protobufjs": "6.8.8",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "swagger2openapi": "4.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.9",
    "install-peers-cli": "^2.1.1",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "postcss-scss": "^1.0.3",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-test-renderer": "^16.2.0",
    "rollup": "^0.56.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.2.8",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-watch": "^4.3.1",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2"
  },
  "jest": {
    "testURL": "http://localhost/"
  }
}
