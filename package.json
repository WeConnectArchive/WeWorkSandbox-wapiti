{
  "name": "@wework/wapiti",
  "version": "0.1.1",
  "description": "React UI for API Documentation supporting OpenAPI and gRPC",
  "keywords": [
    "api",
    "documentation",
    "openapi",
    "rest",
    "grpc",
    "service mesh",
    "kiali"
  ],
  "homepage": "./",
  "bugs": {
    "url": "https://github.com/WeWorkSandbox/wapiti/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeWorkSandbox/wapiti.git"
  },
  "license": "Apache-2.0",
  "author": "WeWork",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build-css": "node-sass src/ --output-style compressed -o src/",
    "build": "npm run build-css && REACT_APP_VERSION=$npm_package_version REACT_APP_NAME=$npm_package_name REACT_APP_GIT_HASH=$(git rev-parse HEAD) GENERATE_SOURCEMAP=false react-scripts build",
    "start": "npm run build-css && REACT_APP_VERSION=$npm_package_version REACT_APP_NAME=$npm_package_name REACT_APP_GIT_HASH=$(git rev-parse HEAD) react-scripts start",
    "test": "npm run build-css && TEST_RUNNER=1 react-scripts test --env=jsdom __tests__",
    "analyze": "source-map-explorer build/static/js/main.*",
    "prettier": "prettier --write \"{src/**/*.{js,jsx,ts,tsx,json,yml,css,scss},travis.yml,*.json}\"",
    "postinstall": "sed -i -E 's#\\[.*eslint-config-react-app.*\\]#['\\'$PWD/.eslintrc\\'']#g' node_modules/react-scripts/config/webpack*.js"
  },
  "dependencies": {
    "js-yaml": "3.13.1",
    "json-refs": "^3.0.13",
    "marked": "0.7.0",
    "protobufjs": "6.8.8",
    "react": "16.9.0",
    "react-dangerous-html": "1.0.0",
    "react-dom": "16.9.0",
    "react-scripts": "3.0.1",
    "swagger2openapi": "^5.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@types/enzyme": "3.1.15",
    "@types/jest": "23.3.10",
    "@types/lodash": "4.14.123",
    "@types/node": "11.11.4",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "babel-core": "6.26.3",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "jest-canvas-mock": "1.1.0",
    "jest-localstorage-mock": "2.3.0",
    "node-sass": "4.12.0",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "prop-types": "15.7.2",
    "source-map-explorer": "1.8.0",
    "typescript": "3.5"
  },
  "resolutions": {},
  "engines": {
    "node": ">=10.0.0 && <=12",
    "npm": ">=6.0.0 ",
    "yarn": ">=1.0.0 "
  },
  "browserslist": [
    ">10%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
